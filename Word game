#include <iostream>
#include <string>
using namespace std;
int main()
{
	string password = "cplusplus";
	string answer;
	do {
		cout << "What is password? ";
		cin >> answer;
		if (password != answer)
		{
			cout << "Try again! \a\n";
		}
	} while (password != answer);
	cout << "You know the password!";
}
